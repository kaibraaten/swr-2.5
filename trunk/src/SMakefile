RM = delete quiet force
OBJ = act_comm.o act_info.o act_move.o act_obj.o act_wiz.o \
	boards.o build.o \
	clans.o colony.o comm.o const.o copyover.o \
	db.o \
	fight.o \
	handler.o \
	imc.o interp.o \
	magic.o makeobjs.o misc.o morespace.o mud_comm.o mud_prog.o \
	nanny.o \
	planets.o player.o reset.o \
	save.o shops.o skillpackage.o skills.o space.o special.o swskills.o \
	tables.o telopt.o track.o \
	update.o \
	vector3_aux.o vote.o

LIBS = swr_support/swr_support.lib netlib:net.lib lib:snprintf.lib lib:z.lib
SWR2 = swr2

all:		$(SWR2)

.c.o:
		$(CC) $*.c

clean:
		$(RM) \#?.o \#?~ $(SWR2)

$(SWR2):	$(OBJ)
		$(CC) ProgramName=$@ Link Library=$(LIBS) Obj $(OBJ)
